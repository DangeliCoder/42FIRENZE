#include <time.h>
#include <stdlib.h>
#include <stdio.h>

#include "libft.h"

int	main(void)
{
	char	c;

	srand((unsigned)time(NULL));
	c = rand() % 128;
	if (ft_isprint(c))
		printf("'%c' is a printable character.\n", c);
	else
		printf("'%c' isn't a printable character.\n", c);
	return (0);
}
